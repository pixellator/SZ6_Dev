# .env.dev  â€“  development environment variables
# Copy to .env and edit as needed.  DO NOT commit .env to git.

DJANGO_SECRET_KEY=dev-secret-key-not-for-production-use
DJANGO_DEBUG=true
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1

# Set to true once PostgreSQL is installed and configured.
USE_POSTGRES=false
USE_REDIS=false

# PostgreSQL connection (used when USE_POSTGRES=true)
UARD_DB_NAME=wsz6_uard
UARD_DB_USER=wsz6
UARD_DB_PASSWORD=wsz6dev
UARD_DB_HOST=localhost
UARD_DB_PORT=5432

GDM_DB_NAME=wsz6_gdm
GDM_DB_USER=wsz6
GDM_DB_PASSWORD=wsz6dev
GDM_DB_HOST=localhost
GDM_DB_PORT=5432

# Redis URL (used when USE_REDIS=true)
REDIS_URL=redis://127.0.0.1:6379

# Internal API shared secret
INTERNAL_API_KEY=dev-internal-key-change-in-production
